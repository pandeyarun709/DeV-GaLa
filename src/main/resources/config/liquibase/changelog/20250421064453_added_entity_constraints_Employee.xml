<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
    <!--
        Added the constraints for entity Employee.
    -->
    <changeSet id="20250421064453-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="employee_id"
                                 baseTableName="rel_employee__qualifications"
                                 constraintName="fk_rel_employee__qualifications__employee_id"
                                 referencedColumnNames="id"
                                 referencedTableName="employee"
                                 />

        <addForeignKeyConstraint baseColumnNames="qualifications_id"
                                 baseTableName="rel_employee__qualifications"
                                 constraintName="fk_rel_employee__qualifications__qualifications_id"
                                 referencedColumnNames="id"
                                 referencedTableName="qualification"
                                 />

        <addForeignKeyConstraint baseColumnNames="department_id"
                                 baseTableName="employee"
                                 constraintName="fk_employee__department_id"
                                 referencedColumnNames="id"
                                 referencedTableName="department"
                                 />

        <addForeignKeyConstraint baseColumnNames="test_id"
                                 baseTableName="employee"
                                 constraintName="fk_employee__test_id"
                                 referencedColumnNames="id"
                                 referencedTableName="diagnostic_test"
                                 />
    </changeSet>
</databaseChangeLog>
